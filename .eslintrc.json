{
  "extends": "next/eslint-config-next",
  "rules": {
    "react/no-unescaped-entities": "warn",
    "react-hooks/rules-of-hooks": "error",
    "@next/next/no-html-link-for-pages": "off",
    "@next/next/no-img-element": "warn",
    "no-console": [
      "warn",
      {
        "allow": ["warn", "error"]
      }
    ]
  }
}
